#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node" --use_strict "$basedir/../xl-json/bin/xl-json" "$@"
  ret=$?
else 
  node --use_strict "$basedir/../xl-json/bin/xl-json" "$@"
  ret=$?
fi
exit $ret
