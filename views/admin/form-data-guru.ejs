    <div class="container-fluid">
      <div class="row mt-5">
        <div class="row">
          <div class="col text-dark text-center mb-5">
            <h3>Tambah Data Guru</h3>
          </div>
        </div>
        <form action="">
          <!-- NIM -->
          <div class="row mb-2">
            <div class="col-md-2 offset-md-3">
              <h5>NIM</h5>
            </div>
            <div class="col-md-4">
              <div class="input-group input-group-sm">
                
                <input type="number" class="form-control rounded-3" aria-describedby="inputGroup-sizing-sm" />
              </div>
            </div>
          </div>

          <!-- Nama Guru -->
          <div class="row mb-2">
            <div class="col-md-2 offset-md-3">
              <h5>Nama Guru</h5>
            </div>
            <div class="col-md-4">
              <div class="input-group input-group-sm">
                
                <input type="text" class="form-control rounded-3" aria-describedby="inputGroup-sizing-sm" />
              </div>
            </div>
          </div>

          <!-- Tempat dan tgl -->
          <div class="row mb-2">
            <div class="col-md-2 offset-md-3">
              <h5>Tempat, Tgl Lahir</h5>
            </div>
            <div class="col-md-2 tmpt-lahir">
              <div class="input-group input-group-sm">
                
                <input type="text" class="form-control rounded-3" placeholder="Tempat" aria-describedby="inputGroup-sizing-sm" />
              </div>
            </div>
            <div class="col-md-2">
              <div class="input-group input-group-sm">
                <input type="date" class="form-control rounded-3" aria-describedby="inputGroup-sizing-sm" />
              </div>
            </div>
          </div>

          <!-- Agama -->
          <div class="row mb-2">
            <div class="col-md-2 offset-md-3">
              <h5>Agama</h5>
            </div>
            <div class="col-md-4">
              <div class="input-group input-group-sm">
                
                <input type="text" class="form-control rounded-3" aria-describedby="inputGroup-sizing-sm" />
              </div>
            </div>
          </div>

          <!-- No tlp -->
          <div class="row mb-2">
            <div class="col-md-2 offset-md-3">
              <h5>No. Tlp</h5>
            </div>
            <div class="col-md-4">
              <div class="input-group input-group-sm">
                
                <input type="number" class="form-control rounded-3" aria-describedby="inputGroup-sizing-sm" />
              </div>
            </div>
          </div>

          <!-- Email -->
          <div class="row mb-2">
            <div class="col-md-2 offset-md-3">
              <h5>Email</h5>
            </div>
            <div class="col-md-4">
              <div class="input-group input-group-sm">
                
                <input type="text" class="form-control rounded-3" aria-describedby="inputGroup-sizing-sm" />
              </div>
            </div>
          </div>

          <!-- Alamat -->
          <div class="row mb-2">
            <div class="col-md-2 offset-md-3">
              <h5>Alamat</h5>
            </div>
            <div class="col-md-4">
              <div class="input-group input-group-sm">
                
                <input type="text" class="form-control rounded-3" aria-describedby="inputGroup-sizing-sm" />
              </div>
            </div>
          </div>

          <!-- Foto -->
          <div class="row mb-2">
            <div class="col-md-2 offset-md-3">
              <h5>Foto</h5>
            </div>
            <div class="col-md-4">
              <div class="input-group input-group-sm">
                
                <input type="file" class="form-control rounded-3" aria-describedby="inputGroup-sizing-sm" />
              </div>
            </div>
          </div>

           <!-- Username -->
           <div class="row mb-2">
            <div class="col-md-2 offset-md-3">
              <h5>Username</h5>
            </div>
            <div class="col-md-4">
              <div class="input-group input-group-sm">
                
                <input type="text" class="form-control rounded-3" aria-describedby="inputGroup-sizing-sm" />
              </div>
            </div>
          </div>

          <!-- Password -->
          <div class="row mb-2">
            <div class="col-md-2 offset-md-3">
              <h5>Password</h5>
            </div>
            <div class="col-md-4">
              <div class="input-group input-group-sm">
                
                <input type="text" class="form-control rounded-3" aria-describedby="inputGroup-sizing-sm" />
              </div>
            </div>
          </div>

          <!-- button -->
          <div class="row mt-4 mb-3">
            <div class="col-md-6 offset-md-3">
              <div class="d-grid justify-content-md-end">
                <a href="data-guru.html">
                  <button type="button" class="btn btn-primary">Tambah</button>
                </a>
              </div>
            </div>
          </div>
        </form>
      </div>
    </div>
